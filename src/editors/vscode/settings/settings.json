{
    "workbench.startupEditor": "newUntitledFile",
    "window.openFoldersInNewWindow": "on",
    "window.newWindowDimensions": "maximized",
    "workbench.editor.enablePreview": false,
    "window.restoreWindows": "folders",
    "workbench.activityBar.visible": false,
    "explorer.openEditors.visible": 0,
    "workbench.colorTheme": "Oceanic Next No Italics",
    "workbench.colorCustomizations": {
      "editor.background": "#1B2B34",
      "sideBar.background": "#1B2B34",
      "statusBar.background": "#1B2B34",
      "tab.inactiveBackground": "#111C22",
      "editorGroupHeader.tabsBackground": "#1B2B34"
    },
    "editor.wordWrap": "on",
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    "editor.minimap.enabled": false,
    "editor.formatOnSave": false,
    "eslint.autoFixOnSave": true,
    "editor.fontFamily": "Hæck",
    "editor.fontSize": 14,
    "editor.fontLigatures": true,
    "extensions.ignoreRecommendations": false,
    "window.zoomLevel": 0,
    "editor.autoIndent": false,
    "editor.fontWeight": "300",
    "terminal.integrated.fontFamily": "Hæck",
    "terminal.integrated.fontSize": 14,
    "clock.dateFormat": "hh:MM",
    "git.decorations.enabled": true,
    "workbench.panel.location": "bottom",
